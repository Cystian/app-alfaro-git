{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/asesores",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|WhatsApp|Discord|Twitterbot|LinkedInBot|Slackbot|TelegramBot|crawler|bot|preview).*"
        }
      ],
      "destination": "/asesores-og.html"
    },
    {
      "source": "/:path((?!api/)(?!assets/)(?!og/)(?!.*\\.).*)",
      "destination": "/"
    }
  ]
}
