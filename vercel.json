{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },

    {
      "src": "/asesores/?",
      "headers": {
        "user-agent": ".*(facebookexternalhit|Facebot|WhatsApp|Discord|Twitterbot|LinkedInBot|Slackbot|TelegramBot|crawler|bot|preview).*"
      },
      "dest": "/asesores-og.html"
    },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
